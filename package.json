  "bugs": {
    "url": "https://github.com/loreanvictor/tmplr-core/issues"
  },
  "homepage": "https://github.com/loreanvictor/tmplr-core#readme",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/change-case": "^2.3.1",
    "@types/jest": "^27.5.1",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-jest": "^28.1.0",
    "eslint": "^8.16.0",
