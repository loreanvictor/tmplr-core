{
  "name": "@tmplr/core",
  "version": "0.5.6",
  "description": "core concepts and commands for tmplr",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "files": [
    "dist/commonjs",
    "dist/es"
  ],
  "sideEffects": false,
  "scripts": {
